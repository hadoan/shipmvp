{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "build",
        "${workspaceFolder}/backend/src/ShipMvp.Api/ShipMvp.Api.csproj"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-apphost",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "build",
        "${workspaceFolder}/backend/src/ShipMvp.AppHost/ShipMvp.AppHost.csproj"
      ],
      "group": "build",
      "problemMatcher": "$msCompile"
    }
  ]
}
